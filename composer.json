{
  "name": "shaotang/autumn",
  "description": "Just another PHP framework.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Autumn\\": "src/autumn/",
      "App\\": "app/src/",
      "Api\\": "api/src/",
      "Admin\\": "admin/src/",
      "Console\\": "console/src/"
    }
  },
  "authors": [
    {
      "name": "Shaotang Zhang",
      "email": "shaotang.zhang@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1.0",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-intl": "*",
    "psr/cache": ">=3.0.0",
    "psr/http-message": ">=1.0.1",
    "psr/http-server-middleware": "1.0.1",
    "psr/simple-cache": ">=3.0.0",
    "psr/event-dispatcher": ">=1.0.0",
    "psr/log": ">=1.1.1",
    "symfony/css-selector": "^6.4",
    "ext-iconv": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
